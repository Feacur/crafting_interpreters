print("> pairs");
class Pair {}

var pair = Pair();
pair.first = 1;
pair.second = 2;
print(pair.first + pair.second);

print("> methods");
class Scone {
	topping(first, second) {
		print("scone with " + first + " and " + second);
	}
}

var scone = Scone();
scone.topping("berries", "cream");

print("> this");
class Nested {
	method() {
		fun function() {
			print(this);
		}

		function();
	}
}
Nested().method();

