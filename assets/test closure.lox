print("> global or local");
var x = "global";
fun outer() {
	var x = "outer";
	fun inner() {
		print(x);
	}
	inner();
}
outer();

print("> closure ");
fun makeClosure(value) {
	fun closure() {
		print(value);
	}
	return closure;
}

var doughnut = makeClosure("doughnut");
var bagel = makeClosure("bagel");
doughnut();
bagel();
