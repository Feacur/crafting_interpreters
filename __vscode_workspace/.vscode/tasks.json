{
	"version": "2.0.0",
	"presentation": {
		"reveal": "never",
		"panel": "shared",
		"showReuseMessage": false,
		"clear": true,
		"revealProblems": "onProblem",
	},
	"options": {"cwd": "../project"},
	"tasks": [
		{
			"label": "Build interpreter",
			"group": "build",
			"type": "process",
			"command": "dotnet",
			"args": [
				"build",
				"interpreter.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"problemMatcher":"$msCompile",
		},
		{
			"label": "Build ast_generator",
			"group": "build",
			"type": "process",
			"command": "dotnet",
			"args": [
				"build",
				"ast_generator.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"problemMatcher":"$msCompile",
		}
	]
}